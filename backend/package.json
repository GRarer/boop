{
  "name": "boop-node-backend",
  "version": "0.0.1",
  "description": "A node-based backend for Boop, likely to be replaced by a different platform after the prototyping stage.",
  "main": "build_output/index.js",
  "scripts": {
    "build": "npx tsc --build",
    "clean": "npx tsc --build --clean",
    "serve": "npm run clean && npm run build && node .",
    "build-watch": "npm run clean && npx tsc --build --watch",
    "serve-watch": "npm run clean && npx tsc --build && npx tsc --build --watch | npx nodemon .",
    "lint": "npx eslint src --ext .ts"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/web-push": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "typescript": "^4.1.3",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "eslint": "^7.17.0",
    "nodemon": "^2.0.6"
  }
}
