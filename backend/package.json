{
  "name": "boop-node-backend",
  "version": "0.0.1",
  "description": "The backend server for Boop, a platform for spontaneous social messaging developed as a Georgia Tech junior design project",
  "license": "MIT",
  "main": "build_output/index.js",
  "scripts": {
    "build": "npx tsc --build",
    "clean": "npx tsc --build --clean",
    "start-node": "node .",
    "serve": "npm run clean && npm run build && node .",
    "build-watch": "npm run clean && npx tsc --build --watch",
    "serve-watch": "npm run clean && npx tsc --build && (trap 'kill 0' SIGINT ; npx tsc --build --watch & npx nodemon .)",
    "lint": "npx eslint src --ext .ts"
  },
  "author": "",
  "dependencies": {
    "@types/web-push": "^3.3.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "boop-core": "file:../core",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.1",
    "luxon": "^1.26.0",
    "minimist": "^1.2.5",
    "pg": "^8.5.1",
    "pg-format": "^1.0.4",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/gravatar": "^1.8.1",
    "@types/luxon": "^1.26.2",
    "@types/minimist": "^1.2.1",
    "@types/pg": "^7.14.7",
    "@types/pg-format": "^1.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.17.0",
    "nodemon": "^2.0.6"
  }
}
